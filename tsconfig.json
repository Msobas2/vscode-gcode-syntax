{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "ES2019",
		"outDir": "out",
		"lib": [
			"ES2019"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"resolveJsonModule": true,
		"newLine": "lf",
		"removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules",
		"samplenc",
	]
}